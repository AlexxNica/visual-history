{
  "name": "Histree",
  "description": "Tree-based browser history management and visualization",
  "version": "0.1",
  "author": "Joel Gustafson",
  "permissions": [
    "tabs",
    "webNavigation",
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": ["keyListeners.js"],
  "commands": {
    "move-up": {
      "suggested_key": {
        "default": "Ctrl+Up"
      },
      "description": "Toggle feature foo"
    },
    "move-down": {
      "suggested_key": {
        "default": "Ctrl+Down"
      },
      "description": "Toggle feature foo"
    },
    "move-left": {
      "suggested_key": {
        "default": "Ctrl+Left"
      },
      "description": "Toggle feature foo"
    },
    "move-right": {
      "suggested_key": {
        "default": "Ctrl+Right"
      },
      "description": "Toggle feature foo"
    }
  },
  "manifest_version": 2
}
